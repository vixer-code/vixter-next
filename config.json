{
  "token_hmac_secret_key": "${TOKEN_HMAC_SECRET_KEY}",
  "api_key": "${API_KEY}",
  "admin": {
    "enabled": true,
    "password": "${ADMIN_PASSWORD}",
    "secret": "${ADMIN_SECRET}"
  },
  "allowed_origins": ["*"],

  "publish": true,
  "subscribe_to_publish": true,
  "presence": true,
  "join_leave": true,
  "history_size": 10,
  "history_ttl": "300s",
  "force_recovery": true,

  "namespaces": [
    {
      "name": "user",
      "publish": false,
      "subscribe_to_publish": false,
      "presence": false,
      "join_leave": false,
      "history_size": 0,
      "history_ttl": "0s"
    },
    {
      "name": "conversation",
      "publish": true,
      "subscribe_to_publish": true,
      "presence": true,
      "join_leave": true,
      "history_size": 50,
      "history_ttl": "3600s",
      "force_recovery": true
    },
    {
      "name": "typing",
      "publish": true,
      "subscribe_to_publish": false,
      "presence": false,
      "join_leave": false,
      "history_size": 0,
      "history_ttl": "0s"
    }
  ]
}